<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-plugin-ios-cookie-manager" version="1.0.0" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <name>cordova-plugin-ios-cookie-manager</name>
    <platform name="ios">
		<config-file target="config.xml" parent="/*">
			<feature name="Cookie">
				<param name="ios-package" value="Cookie" />
			</feature>
		</config-file>
		<header-file src="src/ios/hpd/Cookie.h" />
		<source-file src="src/ios/hpd/Cookie.m" />
	</platform>
</plugin>
